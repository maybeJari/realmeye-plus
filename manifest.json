{
    "manifest_version": 2,

    "name": "Realmeye Plus",
    "version": "1.0.0",
    "description": "Extension to Enhance your Experience on Realmeye!",

    "author": "notJari",

    "homepage_url": "https://twitter.com/maybejari",

    "icons": { 
        "16": "img/logo/16.png",
        "48": "img/logo/48.png",
       "128": "img/logo/128.png"
    },

    "permissions": [ 
        "*://*.realmeye.com/*"
    ],

    "content_scripts": 
    [{
        "css": ["modifications/styles.css"],
        "matches": ["*://*.realmeye.com/*"]
    }],

    "browser_action": {
        "default_popup": "popup/popup.html"
      }
    
}